{
	"compilerOptions": {
		"lib": [
			"es2017"
		],
		"newLine": "lf",
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"sourceMap": false,
		"strict": true,
		"module": "CommonJS",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"target": "ESNext"
	},
}
